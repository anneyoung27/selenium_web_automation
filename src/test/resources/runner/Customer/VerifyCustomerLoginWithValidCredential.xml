<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TC-001 - Customer Login Test with Valid Credential">
    <listeners>
        <listener class-name="listeners.CustomListeners"/>
    </listeners>

    <test name="Customer Login Test">
        <classes>
            <class name="testCases.Customer.CustomerLoginTest"/>
        </classes>
    </test>

    <test name="Customer Page Test">
        <classes>
            <class name="testCases.Customer.CustomerProfilePageTest"/>
        </classes>
    </test>

    <test name="Customer Logout Test">
        <classes>
            <class name="testCases.Customer.CustomerLogoutTest"/>
        </classes>
    </test>
</suite>